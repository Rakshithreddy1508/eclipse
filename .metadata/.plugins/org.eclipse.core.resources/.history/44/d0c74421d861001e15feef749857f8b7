package com.lulu.developmentWork.testCases;
public class BusinessLogicProcess {


	//Business logic devlopment
	public int show() {
		int data;
		int no1;
		int no2;

		no1=3;
		no2=4;
		data= no1+ no2;

		return data;

	}
	public int sub() {
		int data;
		int no1 = 8;
		int no2 = 3;

		
		data= no1 - no2;

		return data;

	}


}