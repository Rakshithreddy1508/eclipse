package Assgiments;





	import java.util.ArrayList;
import java.util.List;

	public class HR_Manager {

	    List<Employee> al = new ArrayList<Employee>();



	    void process_Records() {


	         al.add(new Employee("Harini", "Software Developer", 21));
	         al.add(new Employee("Ajith", "Software Developer", 22));
	         al.add(new Employee("Deepthi", "Digital", 28));
	         al.add(new Employee("Apoorva", "Retail", 24));
	         al.add(new Employee("Keerthana", "Product Management", 23));

	         int v=0;
	         for(Employee e:al) {
	              v++;
	              System.out.println("Emplyee "+v+" Details: "+e);

	         }

	    }

	}
